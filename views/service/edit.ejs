<%- contentFor('HeaderCss') %>
<%- contentFor('breadcrumb') %>
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-flex align-items-center justify-content-between">
            <h4 class="mb-0">Add Services</h4>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="/admin/services">Services</a></li>
                    <li class="breadcrumb-item active">Create</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<%- contentFor('body') %>
<% if (message!="" ) { %>
    <div class="alert alert-success">
        <%= message  %>
    </div>
<% } %>
<% if (error!="") { %>
    <div class="alert alert-danger">
        <%= error %>
    </div>
<% } %>
<!-- <form class="row" action="/admin/service/store" method="post" enctype="multipart/form-data"> -->
<div class="row">
    <div class="col-lg-12">        
        <div class="row">
            <!-- FIRST SECTION START -->
            <div class="col-md-12">
                <form action="/admin/service/firstsection/update" method="post" enctype="multipart/form-data">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">First Section (Banner Section)</h3>
                        </div>
                        <div class="card-body row">
                            <%
                                const firstsection = JSON.parse(service.first_section);
                            %>
                            <div class="col-md-12">
                                <label for="">Service name  <span class="text-danger">*</span></label>
                                <input type="text" class="form-control" name="service_name" required value="<%= service.title %>">
                            </div>
                            <div class="col-md-6">
                                <label class="form-label">Main Title <span class="text-danger">*</span></label>
                                <input type="text" class="form-control" placeholder="Enter main title" name="main_title" required value="<%= firstsection.main_title %>">
                            </div>
                            <div class="col-md-6">
                                <label class="form-label">Sub Title <span class="text-danger">*</span></label>
                                <input type="text" class="form-control" placeholder="Enter sub title" name="subtitle" required value="<%= firstsection.subtitle %>">
                            </div>
                            <div class="col-md-6 mt-3">
                                <label class="form-label">Desktop Banner image<span class="text-danger">*</span></label>
                                <input type="file" class="form-control" name="desktop_banner_image">
                                <% if(firstsection.desktop_banner_image!="" && firstsection.desktop_banner_image != null){ %>
                                    <div class="image-container mt-3"> 
                                        <div class="inner">
                                            <img src="<%= firstsection.desktop_banner_image %>" width="200" />
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                            <div class="col-md-6 mt-3">
                                <label class="form-label">Mobile Banner image<span class="text-danger">*</span></label>
                                <input type="file" class="form-control" name="mobile_banner_image">
                                <% if(firstsection.mobile_banner_image!="" && firstsection.mobile_banner_image != null){ %>
                                    <div class="image-container mt-3"> 
                                        <div class="inner">
                                            <img src="<%= firstsection.mobile_banner_image %>" width="200" />
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                        <div class="card-footer">
                            <div class="row">
                                <div class="col-md-6 text-left">
                                    <a href="/admin/services" class="btn btn-secondary w-md">Back</a>
                                </div>
                                <div class="col-md-6 text-right">
                                    <input type="hidden" name="id" value="<%= service.id %>">
                                    <button type="submit" class="btn btn-primary w-md float-end">Submit</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <!-- FIRST SECTION END -->

