<script src="/assets/libs/@ckeditor/ckeditor5-build-classic/build/ckeditor.js"></script>
<script>
    function addRow() {
        $('#portfolioTable tbody').append(`
            <tr>
                <td>
                    <textarea class="form-control" name="portfolio_points[]" rows="2" required></textarea>
                </td>
                <td>
                    <span class="btn btn-sm btn-success add-btn" onclick="addRow()"></span>
                    <span class="btn btn-sm btn-danger remove-btn" onclick="removeRow(this)"></span>
                </td>
            </tr>
        `);
    }

    // Remove a row
    function removeRow(button) {
        if ($('#portfolioTable tbody tr').length > 1) {
            $(button).closest('tr').remove();
        } else {
            alert('At least one row is required.');
        }
    }
    
    ClassicEditor.create(
        document.querySelector(".second_section_content"),
        {
            ckfinder: {
                uploadUrl: '/admin/page/editor/upload'
            },
        }
    )
    .then(function (editor) {
        console.log('CKEditor initialized successfully:', editor);
        editor.ui.view.editable.element.style.height = "200px";
    })
    .catch(function (error) {
        console.error('Error initializing CKEditor:', error);
    });
</script>