<%- contentFor('HeaderCss') %>
<%- contentFor('breadcrumb') %>

<!-- start page title -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-flex align-items-center justify-content-between">
            <h4 class="mb-0">Service</h4>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="/admin/page/service">Service Section</a></li>
                    <li class="breadcrumb-item active">Second Section</li>
                </ol>
            </div>

        </div>
    </div>
</div>
<!-- end page title -->
<%- contentFor('body') %>
<% if (message!="" ) { %>
    <div class="alert alert-success">
        <%= message  %>
    </div>
<% } %>
<% if (error!="") { %>
    <div class="alert alert-danger">
        <%= error %>
    </div>
<% } %>

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title mb-0">Service Section</h4>
            </div>
            <form action="/admin/servicepage/secondsection/update" method="post" enctype="multipart/form-data">
                <div class="card-body">
                    <%
                        const contentJSONParse = JSON.parse(secondsection.content);
                    %>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="mt-3">
                                <label class="form-label">Enter title One</label>
                                <input type="text" class="form-control" name="title_one" value="<%= contentJSONParse.title_one %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link label one</label>
                                <input type="text" class="form-control" name="link_label_one" value="<%= contentJSONParse.link_label_one %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link one</label>
                                <input type="text" class="form-control" name="link_one" value="<%= contentJSONParse.link_one %>">
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Enter content one</label>
                                <textarea name="content_one" class="form-control" rows="6"><%= contentJSONParse.content_one %></textarea>
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Select image one</label>
                                <input type="file" class="form-control" name="image_one">
                                <% if(contentJSONParse.image_one!="" && contentJSONParse.image_one != null){ %>
                                    <div class="image-container mt-3"> 
                                        <div class="inner">
                                            <img src="<%= contentJSONParse.image_one %>" width="200" />
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="mt-3">
                                <label class="form-label">Enter title two</label>
                                <input type="text" class="form-control" name="title_two" value="<%= contentJSONParse.title_two %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link label two</label>
                                <input type="text" class="form-control" name="link_label_two" value="<%= contentJSONParse.link_label_two %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link two</label>
                                <input type="text" class="form-control" name="link_two" value="<%= contentJSONParse.link_two %>">
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Enter content two</label>
                                <textarea name="content_two" class="form-control" rows="6"><%= contentJSONParse.content_two %></textarea>
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Select image two</label>
                                <input type="file" class="form-control" name="image_two">
                                <% if(contentJSONParse.image_two!="" && contentJSONParse.image_two != null){ %>
                                    <div class="image-container mt-3"> 
                                        <div class="inner">
                                            <img src="<%= contentJSONParse.image_two %>" width="200" />
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="mt-3">
                                <label class="form-label">Enter title three</label>
                                <input type="text" class="form-control" name="title_three" value="<%= contentJSONParse.title_three %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link label three</label>
                                <input type="text" class="form-control" name="link_label_three" value="<%= contentJSONParse.link_label_three %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link three</label>
                                <input type="text" class="form-control" name="link_three" value="<%= contentJSONParse.link_three %>">
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Enter content three</label>
                                <textarea name="content_three" class="form-control" rows="6"><%= contentJSONParse.content_three %></textarea>
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Select image three</label>
                                <input type="file" class="form-control" name="image_three">
                                <% if(contentJSONParse.image_three!="" && contentJSONParse.image_three != null){ %>
                                    <div class="image-container mt-3"> 
                                        <div class="inner">
                                            <img src="<%= contentJSONParse.image_three %>" width="200" />
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="mt-3">
                                <label class="form-label">Enter title four</label>
                                <input type="text" class="form-control" name="title_four" value="<%= contentJSONParse.title_four %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter link label four</label>
                                <input type="text" class="form-control" name="link_label_four" value="<%= contentJSONParse.link_label_four %>">
                            </div>
                            <div class="mt-3">
                                <label class="form-label">Enter image four</label>
                                <input type="text" class="form-control" name="link_four" value="<%= contentJSONParse.link_four %>">
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Enter content four</label>
                                <textarea name="content_four" class="form-control" rows="6"><%= contentJSONParse.content_four %></textarea>
                            </div>
                            <div class="mt-3">
                                <label for="" class="form-label">Select image four</label>
                                <input type="file" class="form-control" name="image_four">
                                <% if(contentJSONParse.image_four!="" && contentJSONParse.image_four != null){ %>
                                    <div class="image-container mt-3"> 
                                        <div class="inner">
                                            <img src="<%= contentJSONParse.image_four %>" width="200" />
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-footer">
                    <div class="row">
                        <div class="col-md-6 text-left">
                            <a href="/admin/page/service" class="btn btn-secondary w-md">Back</a>
                        </div>
                        <div class="col-md-6 text-right">
                            <input type="hidden" name="id" value="<%= secondsection.id %>">
                            <button type="submit" class="btn btn-primary w-md float-end">Submit</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<!-- End Form Layout -->


<%- contentFor('FooterJs') %>
<%- contentFor('BottomJs') %>